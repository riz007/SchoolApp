@model IEnumerable<StudentManagementApp.Models.Admin>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Email
        </th>
        <th>
            Password
        </th>
        <th>
            Date of Birth
        </th>
        <th>
            Phone
        </th>
        <th>
            Enroll Date
        </th>
        <th>
            User Name
        </th>
        <th>
            Street
        </th>
        <th>Action</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.firstName)
